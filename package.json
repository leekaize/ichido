{
  "name": "ichido",
  "version": "2.0.0",
  "description": "A minimalist space for honest reflection",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "journaling",
    "reflection",
    "prayer",
    "mindfulness",
    "writing",
    "minimalist"
  ],
  "author": "Lee Kai Ze",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leekaize/ichido.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
